    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "args": ["https://depth-geneva-triumph-contributions.trycloudflare.com", "generate", "test.json", "outfile.png"],
            "name": "Launch Generate test.json",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/**",
                "!**/node_modules/**"
              ],
            "program": "${workspaceFolder}/index.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "preLaunchTask": "tsc: build - tsconfig.json",
            "console": "integratedTerminal", 
            "outputCapture": "std",
        }
    ]
}